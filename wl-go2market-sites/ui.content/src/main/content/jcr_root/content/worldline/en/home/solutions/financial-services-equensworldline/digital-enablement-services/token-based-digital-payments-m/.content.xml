                                jcr:lastModifiedBy="a480381"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="gotomarket/components/content/richtext"
                                text="&lt;p>&amp;nbsp;&lt;/p>&#xa;&lt;ul>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>End-to-end services resulting in reduced project costs and integration impacts&lt;/span>&lt;/li>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>Major scale deployments, multi-region and multi-tenant support&lt;/span>&lt;/li>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>High degree of configurability and flexibility&lt;/span>&lt;/li>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>Future-proof deployment models using hosted modules and API approach&lt;/span>&lt;/li>&#xa;&lt;/ul>&#xa;"
                                textIsRich="true"/>
                        </column2>

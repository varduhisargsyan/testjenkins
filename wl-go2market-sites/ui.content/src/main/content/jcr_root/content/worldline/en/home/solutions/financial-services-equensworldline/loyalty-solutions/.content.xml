                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="gotomarket/components/content/articlebriefitem"
                                alignment="v"
                                isDateHidden="true"
                                isExternalUrl="true"
                                isLinkHidden="false"
                                isSubtitleHidden="false"
                                isTitleHidden="false"
                                linkText="Download our brochure"
                                noExtention="true"
                                subtitle="A ready-to-run solution that can be fully tailored to your needs"
                                text="Bank' customer bases hold valuable elements to build a fruitful virtual marketplace. Their opt-in cardholders will be able to receive targeted promotions from affiliated merchants receiving coupons and cash-back.&#xd;&#xa;Interested?"
                                title="brochure"
                                url="/content/dam/equensworldline/documents/marketing-collaterals/WL-Cash-club.pdf"/>
                        </column2>
                    </grid_914252526>

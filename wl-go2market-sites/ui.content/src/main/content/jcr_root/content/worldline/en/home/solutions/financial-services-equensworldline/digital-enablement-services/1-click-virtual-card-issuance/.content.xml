                                sling:resourceType="gotomarket/components/content/richtext"
                                text="&lt;ul>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>Seamless mobile payment services, ensuring consumer convenience&lt;/span>&lt;/li>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>Easy integration and minimal impact on your banking environment&lt;/span>&lt;/li>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>Short time-to-market&lt;/span>&lt;/li>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>Ensures risk mitigation through secure and robust systems and processes&lt;/span>&lt;/li>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>Future-proof business partner that smartly combines the strengths of several business partners and suppliers with a clear vision and roadmap &lt;/span>&lt;/li>&#xa;&lt;/ul>&#xa;"

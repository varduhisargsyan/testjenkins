                                showDots="false"
                                title="Value for our clients"
                                titleAlignment="left"/>
                            <image
                                jcr:created="{Date}2017-09-27T15:51:31.224+02:00"
                                jcr:createdBy="evv"
                                jcr:lastModified="{Date}2017-09-27T15:51:48.631+02:00"

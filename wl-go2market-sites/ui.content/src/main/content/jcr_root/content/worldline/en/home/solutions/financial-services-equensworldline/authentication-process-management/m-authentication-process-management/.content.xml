                            <richtext_422646475_c
                                jcr:created="{Date}2017-09-13T13:33:33.551+02:00"
                                jcr:createdBy="a480019"
                                jcr:lastModified="{Date}2017-09-14T16:15:16.237+02:00"
                                jcr:lastModifiedBy="a480019"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="gotomarket/components/content/richtext"
                                text="&lt;ul>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>Manages authentication workflows for all use cases: Card-based remote payments (3D Secure), e-banking and PSD2/XS2A (AISP and PISP)&lt;/span>&lt;/li>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>Manages exemption rules&lt;/span>&lt;/li>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>Manages list of trusted beneficiaries for each cardholder&lt;/span>&lt;/li>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>Manages authentication methods for each user, and for each use case&lt;/span>&lt;/li>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>User enrolment&lt;/span>&lt;/li>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>History of last authentications, with the context of each transaction, and justification of exemptions&lt;/span>&lt;/li>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>Statistics, reporting by authentication method and use case&lt;/span>&lt;/li>&#xa;&lt;/ul>&#xa;"
                                textIsRich="true"/>
                        </column1>
                        <column2
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="gotomarket/components/parsys">

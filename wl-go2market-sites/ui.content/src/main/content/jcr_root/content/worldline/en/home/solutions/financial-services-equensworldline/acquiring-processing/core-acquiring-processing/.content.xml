                                jcr:lastModifiedBy="a480019"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="gotomarket/components/content/richtext"
                                text="&lt;ul>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>Increase revenue by supporting the international expansion of your merchants&lt;/span>&lt;/li>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>Rely on highly scalable acquiring processing solution that flexibly supports fluctuations in transaction volumes and business growth&lt;/span>&lt;/li>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>Cost-efficient because of flexibility and choice in services to outsource&lt;/span>&lt;/li>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>Acquirer branded online support enabling you to focus on new business&lt;/span>&lt;/li>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>Services compliant to European regulations&lt;/span>&lt;/li>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>Short time-to-market because of modular service portfolio&lt;/span>&lt;/li>&#xa;&lt;/ul>&#xa;"

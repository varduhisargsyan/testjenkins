                                sling:resourceType="gotomarket/components/content/richtext"
                                text="&lt;ul>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>Consolidated multi-country &amp;amp; multi-currency processing and reporting&lt;/span>&lt;/li>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>Centralized transaction processing for merchants&lt;/span>&lt;/li>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>Combined transaction clearing for all targeted countries&lt;/span>&lt;/li>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>Extranet to manage merchants’ transactions&lt;/span>&lt;/li>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>Trusted fraud prevention&lt;/span>&lt;/li>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>Unified interface to access multiple card schemes&lt;/span>&lt;/li>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>Fully owned acquiring services, own software assets implemented&lt;/span>&lt;/li>&#xa;&lt;/ul>&#xa;"
                                textIsRich="true"/>
                        </column1>

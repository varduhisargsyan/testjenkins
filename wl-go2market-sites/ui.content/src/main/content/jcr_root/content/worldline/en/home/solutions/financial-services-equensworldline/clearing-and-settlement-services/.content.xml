                                fileReference="/content/dam/equensworldline/images/articles/ew_article_034.jpg"/>
                            <richtext_copy
                                jcr:created="{Date}2017-09-09T02:19:21.107+02:00"
                                jcr:createdBy="evv"
                                jcr:lastModified="{Date}2017-09-24T17:03:47.748+02:00"
                                jcr:lastModifiedBy="evv"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="gotomarket/components/content/richtext"
                                text="&lt;ul>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>Lower your liquidity risk and improve revenues&lt;/span>&lt;/li>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>Benefit from lower settlement account balance with our 30-minute settlement cycle&lt;/span>&lt;/li>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>Connect to the whole of Europe through a single point of contact&lt;/span>&lt;/li>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>Reduce transaction costs and simplify SEPA compliance&lt;/span>&lt;/li>&#xa;&lt;/ul>&#xa;"

                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="gotomarket/components/content/stringplaceholder"
                                text="&lt;p>&amp;lt;h3&amp;gt;&lt;br>&#xa;Worldline share price&amp;lt;/h3&amp;gt;&lt;br>&#xa;&amp;nbsp;&lt;br>&#xa;&lt;/p>&#xa;&lt;p>&amp;lt;link href=&amp;quot;/content/dam/worldline/js/investors/shareprice.css&amp;quot; rel=&amp;quot;stylesheet&amp;quot; media=&amp;quot;screen&amp;quot; type=&amp;quot;text/css&amp;quot;/&amp;gt;&lt;/p>&#xa;&lt;p>&amp;nbsp;&lt;/p>&#xa;&lt;p>&amp;lt;script&amp;gt; var Browser = {isIE:false};&amp;lt;/script&amp;gt;&amp;lt;!-- By default we will assume that the browser is not IE --&amp;gt;&lt;/p>&#xa;&lt;p>&amp;lt;!--[if IE 5]&amp;gt;&amp;lt;script&amp;gt;Browser = {isIE:true,version:5};&amp;lt;/script&amp;gt;&amp;lt;![endif]--&amp;gt;&lt;/p>&#xa;&lt;p>&amp;lt;!--[if IE 6]&amp;gt;&amp;lt;script&amp;gt;Browser = {isIE:true,version:6};&amp;lt;/script&amp;gt;&amp;lt;![endif]--&amp;gt;&lt;/p>&#xa;&lt;p>&amp;lt;!--[if IE 7]&amp;gt;&amp;lt;script&amp;gt;Browser = {isIE:true,version:7};&amp;lt;/script&amp;gt;&amp;lt;![endif]--&amp;gt;&lt;/p>&#xa;&lt;p>&amp;lt;!--[if IE 8]&amp;gt;&amp;lt;script&amp;gt;Browser = {isIE:true,version:8};&amp;lt;/script&amp;gt;&amp;lt;![endif]--&amp;gt;&lt;/p>&#xa;&lt;p>&amp;lt;!--[if IE 9]&amp;gt;&lt;/p>&#xa;&lt;p>&amp;lt;div style=&amp;quot;font-size:120%;margin-left:10px;&amp;quot;&amp;gt;&amp;lt;b&amp;gt;Share price:&amp;lt;/b&amp;gt; &amp;lt;br /&amp;gt;&amp;lt;b&amp;gt;&amp;lt;font id=&amp;quot;colordynid&amp;quot; style=&amp;quot;color:#cd1f3d;&amp;quot;&amp;gt;€ &amp;lt;span id=&amp;quot;collected0&amp;quot;&amp;gt;Loading...&amp;lt;/span&amp;gt;&amp;lt;/font&amp;gt;&amp;lt;/b&amp;gt;&amp;lt;img id=&amp;quot;picid&amp;quot; class=&amp;quot;updownpic&amp;quot; src=&amp;quot;/content/dam/worldline/js/investors/images/stock-price-down.gif&amp;quot; /&amp;gt;&amp;lt;br /&amp;gt;&amp;lt;b&amp;gt;Change:&amp;lt;/b&amp;gt; &amp;lt;span id=&amp;quot;collected1&amp;quot;&amp;gt;Loading...&amp;lt;/span&amp;gt;%&amp;lt;br /&amp;gt;&amp;lt;font class=&amp;quot;date&amp;quot;&amp;gt;&amp;lt;span id=&amp;quot;collected2&amp;quot;&amp;gt;Loading...&amp;lt;/span&amp;gt; - &amp;lt;span id=&amp;quot;collected3&amp;quot;&amp;gt;Loading...&amp;lt;/span&amp;gt; CET&amp;lt;/font&amp;gt;&amp;lt;/div&amp;gt;&lt;/p>&#xa;&lt;p>&amp;lt;script&amp;gt;Browser = {isIE:true,version:9};&amp;lt;/script&amp;gt;&lt;/p>&#xa;&lt;p>&amp;lt;![endif]--&amp;gt;&lt;/p>&#xa;&lt;p>&amp;nbsp;&lt;/p>&#xa;&lt;p>&amp;lt;script type=&amp;quot;text/javascript&amp;quot;&amp;gt;&lt;/p>&#xa;&lt;p>if (Browser.isIE){&lt;/p>&#xa;&lt;p>&amp;nbsp; &amp;nbsp; if (Browser.version &amp;lt;= 9){&lt;/p>&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;lt;!--alert(&amp;quot;IE&amp;quot;); --&amp;gt;&lt;/p>&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; var xdr; &amp;nbsp; &amp;nbsp;&lt;/p>&#xa;&lt;p>&amp;nbsp;&lt;/p>&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; function loadFile(file) {&lt;/p>&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; xdr = new XDomainRequest();&lt;/p>&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; xdr.onload = function() {&lt;/p>&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; var data = xdr.responseText;&lt;/p>&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;&amp;nbsp;&lt;/p>&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; collected=data.split(&amp;quot;;&amp;quot;); &amp;nbsp; //parses the data delimited by comma and put data into array&lt;/p>&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;&amp;nbsp;&lt;/p>&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; var signedyn=parseFloat(collected[1]);&lt;/p>&#xa;&lt;p>&amp;nbsp;&lt;/p>&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; if (signedyn &amp;gt;= 0) {&lt;/p>&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; document.getElementById('colordynid').style.color &amp;nbsp;= '#9bbe3b';&lt;/p>&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; document.getElementById('picid').src &amp;nbsp;= '/content/dam/worldline/js/investors/images/stock-price-up.gif';&lt;/p>&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; }&lt;/p>&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;&amp;nbsp;&lt;/p>&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; collected[0]= collected[0].substring(4,8);&lt;/p>&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;&amp;nbsp;&lt;/p>&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; document.getElementById('collected0').innerText = collected[0]; //price&lt;/p>&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; document.getElementById('collected1').innerText = collected[1]; //change&lt;/p>&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; document.getElementById('collected2').innerText = collected[2]; //date&lt;/p>&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; document.getElementById('collected3').innerText = collected[3]; //time&lt;/p>&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;&lt;/p>&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; }&lt;/p>&#xa;&lt;p>&amp;nbsp; &amp;nbsp;&amp;nbsp;&lt;/p>&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; xdr.open(&amp;quot;GET&amp;quot;, &amp;quot;https://charts.bfi.aw.atosorigin.com/fr/corporate/worldline/sharesPrice.html&amp;quot;);&lt;/p>&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; xdr.send();&lt;/p>&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; }&lt;/p>&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; loadFile();&lt;/p>&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;&lt;/p>&#xa;&lt;p>&amp;nbsp; &amp;nbsp; }&lt;/p>&#xa;&lt;p>}&lt;/p>&#xa;&lt;p>&amp;lt;/script&amp;gt;&lt;/p>&#xa;&lt;p>&amp;nbsp;&lt;/p>&#xa;&lt;p>&amp;nbsp;&lt;/p>&#xa;&lt;p>&amp;lt;script&amp;gt;&lt;/p>&#xa;&lt;p>if (!Browser.isIE){&lt;/p>&#xa;&lt;p>&amp;nbsp;&lt;/p>&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;&amp;nbsp;&lt;/p>&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp; function loadFile(file) {&lt;/p>&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;&amp;nbsp;&lt;/p>&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; var xhr = new XMLHttpRequest();&lt;/p>&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; // On souhaite juste récupérer le contenu du fichier, la méthode GET suffit amplement :&lt;/p>&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; // xhr.open('GET', 'http://qlf-charts.infofi.aw.atosorigin.com/fr/corporate/worldline/sharesPrice.html');&lt;/p>&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; xhr.open('GET', 'https://charts.bfi.aw.atosorigin.com/fr/corporate/worldline/sharesPrice.html');&lt;/p>&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; xhr.onreadystatechange = function() { // On gère ici une requête asynchrone&lt;/p>&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; if (xhr.readyState == 4 &amp;amp;&amp;amp; xhr.status == 200) { // Si le fichier est chargé sans erreur&lt;/p>&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; var data = xhr.responseText;&lt;/p>&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; var colordyn =&amp;quot;sharedown&amp;quot;;&lt;/p>&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; var imgdynup ='&amp;lt;img class=&amp;quot;updownpic&amp;quot; src=&amp;quot;/content/dam/worldline/js/investors/images/stock-price-up.gif&amp;quot; &amp;nbsp;/&amp;gt;';&lt;/p>&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; var imgdyndown ='&amp;lt;img class=&amp;quot;updownpic&amp;quot; &amp;nbsp;src=&amp;quot;/content/dam/worldline/js/investors/images/stock-price-down.gif&amp;quot; &amp;nbsp;/&amp;gt;';&lt;/p>&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; var imgdyn = imgdyndown;&lt;/p>&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;&amp;nbsp;&lt;/p>&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; collected=data.split(&amp;quot;;&amp;quot;); &amp;nbsp; //parses the data delimited by comma and put data into array&lt;/p>&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;&amp;nbsp;&lt;/p>&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; var signedyn=parseFloat(collected[1]);&lt;/p>&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; if (signedyn &amp;gt;= 0) {colordyn =&amp;quot;shareup&amp;quot;; imgdyn = imgdynup; }&lt;/p>&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;&amp;nbsp;&lt;/p>&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;document.getElementById('fileContent').innerHTML = '&amp;lt;span&amp;gt;&amp;lt;b&amp;gt;Share price: &amp;lt;br /&amp;gt;&amp;lt;font class=&amp;quot;'+colordyn+'&amp;quot;&amp;gt;€'+collected[0]+'&amp;lt;/b&amp;gt;&amp;lt;/font&amp;gt;'+imgdyn+'&amp;lt;br /&amp;gt;&amp;lt;b&amp;gt;Change:&amp;lt;/b&amp;gt; '+ collected[1]+'%&amp;lt;br /&amp;gt;&amp;lt;font class=&amp;quot;date&amp;quot;&amp;gt;'+ collected[2]+' - '+ collected[3] + ' CET&amp;lt;/font&amp;gt;&amp;lt;/span&amp;gt;'; // Et on affiche !&lt;/p>&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;&amp;nbsp;&lt;/p>&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; //document.getElementById('fileContent').innerHTML = '&amp;lt;span&amp;gt;' + xhr.responseText + '&amp;lt;/span&amp;gt;'; // Et on affiche !&lt;/p>&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; }&lt;/p>&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; }&lt;/p>&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; xhr.send(null); // La requête est prête, on envoie tout !&lt;/p>&#xa;&lt;p>&amp;nbsp;&lt;/p>&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp; }&lt;/p>&#xa;&lt;p>&amp;nbsp;&lt;/p>&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp; (function() { // Comme d'habitude, une fonction anonyme pour éviter les variables globales&lt;/p>&#xa;&lt;p>&amp;nbsp;&lt;/p>&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; var inputs = document.getElementsByTagName('input'),&lt;/p>&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; inputsLen = inputs.length; &amp;nbsp; &amp;nbsp;&amp;nbsp;&lt;/p>&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;&amp;nbsp;&lt;/p>&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; for (var i = 0 ; i &amp;lt; inputsLen ; i++) {&lt;/p>&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; inputs[i].onclick = function() {&lt;/p>&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; loadFile(this.value); // À chaque clique, un fichier sera chargé dans la page&lt;/p>&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; };&lt;/p>&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; }&lt;/p>&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp; })();&lt;/p>&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp;&amp;nbsp;&lt;/p>&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp; loadFile();&lt;/p>&#xa;&lt;p>&amp;nbsp;&lt;/p>&#xa;&lt;p>}&lt;/p>&#xa;&lt;p>&amp;lt;/script&amp;gt;&lt;/p>&#xa;&lt;p>&amp;nbsp;&lt;/p>&#xa;&lt;p>&amp;lt;p id=&amp;quot;fileContent&amp;quot;&amp;gt;&lt;/p>&#xa;&lt;p>&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;lt;span&amp;gt;&amp;amp;nbsp;&amp;lt;/span&amp;gt;&lt;/p>&#xa;&lt;p>&amp;lt;/p&amp;gt;&lt;/p>&#xa;&lt;p>&amp;nbsp;&lt;/p>&#xa;">

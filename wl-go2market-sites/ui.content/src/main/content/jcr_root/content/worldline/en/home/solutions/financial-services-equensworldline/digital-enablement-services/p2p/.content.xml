                            <richtext_839334717
                                jcr:created="{Date}2017-09-13T13:33:07.188+02:00"
                                jcr:createdBy="a480019"
                                jcr:lastModified="{Date}2017-09-24T12:26:33.794+02:00"
                                jcr:lastModifiedBy="evv"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="gotomarket/components/content/richtext"
                                text="&lt;ul>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>&lt;b>Mobile P2P Payment App or integration of our P2P service with the end-user interface of your choice&lt;/b> to provide a mobile application for Android and iOS, with a user interface for end users of the issuing bank and the corresponding backend. Through our P2P APIs, the service can also be integrated into an existing mobile banking app&lt;/span>&lt;/li>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>The&lt;b> Alias Conversion Service&lt;/b> converts an alias (mobile phone number) into an IBAN and manages the corresponding database&lt;/span>&lt;/li>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>&lt;b>Payment Processing&lt;/b> provides money transfers from one SEPA bank account to another by using a two-step approach (an ePayment transaction (like iDEAL) and consecutive on-us SEPA credit transfer). It provides an interim solution for payment processing and payment authorization until Instant Payments and Access to Account (X2A) are available&lt;/span>&lt;/li>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>&lt;b>Cash Management&lt;/b> provides liquidity management and reconciliation between intermediary bank accounts and the ePayment merchant account&lt;/span>&lt;/li>&#xa;&lt;/ul>&#xa;"
                                textIsRich="true"/>
                        </column1>
                        <column2
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="gotomarket/components/parsys">
                            <titleh2
                                jcr:created="{Date}2017-09-13T11:11:14.391+02:00"

                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="gotomarket/components/content/richtext"
                                text="&lt;ul>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>Banks’ corporate clients can use the online payment method within their websites to optimize the digital user experience and conversion rate&lt;/span>&lt;/li>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>Minimal effort for banks to integrate the solution&lt;/span>&lt;/li>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>Maximum support for the banks’ corporate clients and their service providers, thanks to a self-service portal and integrated test functionality&lt;/span>&lt;/li>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>Use of a trusted, secure, fully compliant network with 24/7/365 availability&lt;/span>&lt;/li>&#xa;&lt;/ul>&#xa;"

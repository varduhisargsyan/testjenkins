                                jcr:lastModifiedBy="evv"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="gotomarket/components/content/richtext"
                                text="&lt;ul>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>Meet current market expectations and accelerate innovations using open APIs&lt;/span>&lt;/li>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>Increase your revenue with new fast, innovative and convenient digital service solutions&lt;/span>&lt;/li>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>Save costs by using shared knowledge&lt;/span>&lt;/li>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>Have a faster time to market due to standardized API interfaces and shop approach&lt;/span>&lt;/li>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>Retrieve real-time data easily and always have the latest data&amp;nbsp;for your application&lt;/span>&lt;/li>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>Meet regulatory requirements&lt;/span>&lt;/li>&#xa;&lt;/ul>&#xa;"

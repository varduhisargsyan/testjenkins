                                sling:resourceType="gotomarket/components/content/richtext"
                                text="&lt;p style=&quot;text-align: center;&quot;>&lt;img src=&quot;/content/dam/worldline-fairs/evex/2017/images/evex2017-162px.png&quot;>&lt;/p>&#xa;&lt;p style=&quot;text-align: center;&quot;>2017 Gold Sponsor&lt;/p>&#xa;&lt;h2>Meet our experts&lt;/h2>&#xa;&lt;p style=&quot;text-align: center;&quot;>&lt;a href=&quot;mailto:coralie.penin@worldline.com&quot; target=&quot;_blank&quot;>&lt;img src=&quot;/content/dam/worldline-fairs/evex/2017/images/book.png&quot;>&lt;/a>&lt;a href=&quot;mailto:jb@vending-europe.eu&quot; target=&quot;_blank&quot;>&lt;img src=&quot;/content/dam/worldline-fairs/evex/2017/images/free.png&quot;>&lt;/a>&lt;/p>&#xa;&lt;h2>Practical information&lt;/h2>&#xa;&lt;p>&lt;b>From Thursday 23rd&lt;/b>&lt;/p>&#xa;&lt;p>&lt;b>to Friday 24th&amp;nbsp; November 2017&lt;/b>&lt;/p>&#xa;&lt;p>&lt;b>&lt;br>&#xa;&lt;/b>Radisson Blu&lt;br>&#xa;Via Filippo Turati 171,&lt;br>&#xa;00185 Rome, Italy&lt;/p>&#xa;&lt;p>&lt;a href=&quot;http://www.vending-europe.eu/en/events/evex/evex-2017/&quot; target=&quot;_blank&quot;>Event website&lt;/a>&lt;/p>&#xa;&lt;p>&amp;nbsp;&lt;/p>&#xa;&lt;p>&amp;nbsp;&lt;/p>&#xa;"

                                jcr:mixinTypes="[cq:LiveRelationship]"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="gotomarket/components/content/sixtyfortycomponents/listtoggle"
                                list="[{&quot;acardion&quot;:&quot;APIs&quot;\,&quot;text&quot;:&quot;Our APIs allow you to manage the full lifecycle of token-based payments\, offering functionalities such as:\\n&lt;ul>\\n&lt;li>&lt;span class=\\&quot;styled\\&quot;>Eligibility checking&lt;/span>\\n&lt;li>&lt;span class=\\&quot;styled\\&quot;>User enrollment&lt;/span>\\n&lt;li>&lt;span class=\\&quot;styled\\&quot;>Provisioning and activation&lt;/span>\\n&lt;li>&lt;span class=\\&quot;styled\\&quot;>Lifecycle management&lt;/span>\\n&lt;li>&lt;span class=\\&quot;styled\\&quot;>Notifications and user messages&lt;/span>\\n&lt;li>&lt;span class=\\&quot;styled\\&quot;>Reporting and history&lt;/span>\\n&lt;/ul>&quot;},{&quot;acardion&quot;:&quot;Default modules&quot;\,&quot;text&quot;:&quot;&lt;ul>\\n&lt;li>&lt;span class=\\&quot;styled\\&quot;>\\nSoftware Development Kit (SDK)&lt;/span>&lt;/li>\\n&lt;li>&lt;span class=\\&quot;styled\\&quot;>\\nToken Requestor Server&lt;/span>&lt;/li>\\n&lt;li>&lt;span class=\\&quot;styled\\&quot;>\\nExtranet tool&lt;/span>&lt;/li>\\n&lt;/ul>\\n&quot;},{&quot;acardion&quot;:&quot;Optional modules&quot;\,&quot;text&quot;:&quot;&lt;ul>\\n&lt;li>&lt;span class=\\&quot;styled\\&quot;>\\nWhite label app&lt;/span>&lt;/li>\\n&lt;li>&lt;span class=\\&quot;styled\\&quot;>\\nToken vault&lt;/span>&lt;/li>\\n&lt;li>&lt;span class=\\&quot;styled\\&quot;>\\nTransaction validation&lt;/span>&lt;/li>\\n&lt;li>&lt;span class=\\&quot;styled\\&quot;>\\nData digitization module&lt;/span>&lt;/li>\\n&lt;li>&lt;span class=\\&quot;styled\\&quot;>\\nWallet functionalities&lt;/span>&lt;/li>\\n&lt;/ul>&quot;}]"/>

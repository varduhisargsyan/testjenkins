                                jcr:lastModifiedBy="a480019"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="gotomarket/components/content/richtext"
                                text="&lt;p>Despite obvious reasons for concern banks may have, PSD2 also represents a unique strategic opportunity for them. &lt;/p>&#xa;"
                                textIsRich="true"/>
                            <listtoggle

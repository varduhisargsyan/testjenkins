                        list="[{&quot;columnSize&quot;:&quot;grid-col-5&quot;},{&quot;columnSize&quot;:&quot;grid-col-7&quot;}]">
                        <column1
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="gotomarket/components/parsys">
                            <articlebriefitem_cop
                                jcr:created="{Date}2017-09-22T18:21:06.275+02:00"
                                jcr:createdBy="a480019"
                                jcr:lastModified="{Date}2017-10-06T13:51:06.604+02:00"
                                jcr:lastModifiedBy="a480019"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="gotomarket/components/content/articlebriefitem"
                                alignment="v"
                                isDateHidden="true"
                                isExternalUrl="true"
                                isLinkHidden="false"
                                isSubtitleHidden="true"
                                isTitleHidden="false"
                                linkText="Read blog article"
                                noExtention="true"
                                text="A lot has already been said and written about PSD2, Access to Accounts and the impact of this new regulation on the payments industry. &#xd;&#xa;&#xd;&#xa;Alessandro Baroni, CMO of equensWorldline, elaborates on this hot topic in an extensive Q&amp;A interview, giving his vision on some crucial aspects of PSD2"
                                title="Will PSD2 bring the financial industry into a new era? "

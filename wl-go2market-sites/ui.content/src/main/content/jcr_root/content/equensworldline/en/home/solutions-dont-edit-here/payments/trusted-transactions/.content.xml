                                sling:resourceType="gotomarket/components/content/titleH2"
                                showDots="false"
                                title="Address the complexity and growing competitiveness of the e-commerce world"
                                titleAlignment="left"/>
                            <richtext
                                jcr:created="{Date}2017-09-09T02:26:36.280+02:00"

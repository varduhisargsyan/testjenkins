                                showDots="false"
                                title="Value for our clients"
                                titleAlignment="left"/>
                            <image
                                cq:lastRolledout="{Date}2017-10-30T17:55:42.302+01:00"
                                cq:lastRolledoutBy="msm-service"
                                jcr:created="{Date}2017-09-27T15:27:32.479+02:00"
                                jcr:createdBy="evv"
                                jcr:lastModified="{Date}2017-10-26T18:28:04.874+02:00"

                                jcr:mixinTypes="[cq:LiveRelationship]"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="foundation/components/image"
                                fileReference="/content/dam/equensworldline/images/articles/ew_article_021.jpg"/>
                            <richtext_422646475
                                cq:lastRolledout="{Date}2017-10-30T17:55:42.303+01:00"
                                cq:lastRolledoutBy="msm-service"
                                jcr:created="{Date}2017-09-13T13:33:33.551+02:00"
                                jcr:createdBy="a480019"
                                jcr:lastModified="{Date}2017-10-26T18:28:04.876+02:00"
                                jcr:lastModifiedBy="evv"
                                jcr:mixinTypes="[cq:LiveRelationship]"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="gotomarket/components/content/richtext"
                                text="&lt;ul>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>Quick time-to-market through easy integration of P2P services into the environment of your choice or using our white label app&lt;/span>&lt;/li>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>Proven solution based on an ePayment or card-based funding method of choice&lt;/span>&lt;/li>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>No changes required in the banking backend&lt;/span>&lt;/li>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>Easily extendable through a modular approach&lt;/span>&lt;/li>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>Easy transition of the service to XS2A and Instant Payment via a clearly defined migration path&lt;/span>&lt;/li>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>Door opener for Instant Payments for end users, as the Mobile P2P Payment Service offers one of the main use cases for Instant Payments already in place today&lt;/span>&lt;/li>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>Banks can reach new markets, currently targeted mostly by FinTech startups with innovative payment services&lt;/span>&lt;/li>&#xa;&lt;/ul>&#xa;"
                                textIsRich="true"/>
                        </column2>
                    </grid>
                </items>

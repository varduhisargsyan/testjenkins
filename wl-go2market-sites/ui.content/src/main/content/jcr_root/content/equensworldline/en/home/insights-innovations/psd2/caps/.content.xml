                jcr:primaryType="nt:unstructured"
                sling:resourceType="gotomarket/components/layout/sectionoffwhite">
                <items
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="gotomarket/components/parsys">
                    <grid
                        jcr:created="{Date}2017-09-23T21:11:52.709+02:00"
                        jcr:createdBy="a480019"
                        jcr:lastModified="{Date}2017-09-23T21:12:15.842+02:00"

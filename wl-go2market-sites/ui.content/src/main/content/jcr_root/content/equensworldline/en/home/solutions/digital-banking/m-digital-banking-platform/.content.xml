                                sling:resourceType="gotomarket/components/content/richtext"
                                text="&lt;ul>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>Renew the customer experience with new value-added services, and create a unique customer experience whatever the digital channel is&lt;/span>&lt;/li>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>Define the best digital holistic approach between web, apps, and branches&lt;/span>&lt;/li>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>Mutualize the digital channel development costs&lt;/span>&lt;/li>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>Leverage the core banking capabilities and provide innovating and optional services at the digital momentum&lt;/span>&lt;/li>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>Capture the PSD2 opportunities and be PSD2-compliant on time&lt;/span>&lt;/li>&#xa;&lt;/ul>&#xa;"

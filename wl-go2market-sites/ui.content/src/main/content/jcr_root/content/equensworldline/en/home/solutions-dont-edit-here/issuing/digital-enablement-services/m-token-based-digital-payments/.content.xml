                                sling:resourceType="gotomarket/components/content/richtext"
                                text="&lt;ul>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>Add-on modules to reduce impact on users and increase security&lt;/span>&lt;/li>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>Live components that have been tested with major European acceptance networks&lt;/span>&lt;/li>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>We take away the integration burden: we have the skills, expertise and flexibility to adapt to local needs&lt;/span>&lt;/li>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>Embedded authentication, ready for PSD2 &lt;/span>&lt;br>&#xa;&lt;/li>&#xa;&lt;/ul>&#xa;"

<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:crx="http://www.day.com/crx/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:rep="internal"
    jcr:primaryType="rep:ACL">
    <allow
        jcr:primaryType="rep:GrantACE"
        rep:principalName="equensworldline.com-editor"
        rep:privileges="{Name}[jcr:versionManagement,crx:replicate,rep:write,jcr:lockManagement]"/>
    <allow0
        jcr:primaryType="rep:GrantACE"
        rep:principalName="equensworldline.com-editor"
        rep:privileges="{Name}[jcr:removeChildNodes,jcr:removeNode,jcr:addChildNodes,jcr:nodeTypeManagement]">
        <rep:restrictions
            jcr:primaryType="rep:Restrictions"
            rep:glob="*/jcr:content*"/>
    </allow0>
    <deny
        jcr:primaryType="rep:DenyACE"
        rep:principalName="content-authors"
        rep:privileges="{Name}[jcr:versionManagement,rep:write,jcr:lockManagement]"/>
    <deny0
        jcr:primaryType="rep:DenyACE"
        rep:principalName="content-authors"
        rep:privileges="{Name}[jcr:removeChildNodes,jcr:removeNode,jcr:addChildNodes,jcr:nodeTypeManagement]">
        <rep:restrictions
            jcr:primaryType="rep:Restrictions"
            rep:glob="*/jcr:content*"/>
    </deny0>
</jcr:root>

                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="gotomarket/components/content/richtext"
                                text="&lt;ul>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>Banks’ corporate clients can optimize incoming revenues from direct debits using the widely accepted e-Mandate solution&lt;/span>&lt;/li>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>New revenues from e-Mandate services provided to corporates and payment service providers&lt;/span>&lt;/li>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>Minimal effort for banks, since no integration is required&lt;/span>&lt;/li>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>Maximum support for the banks’ corporate clients and their service providers with a self-service portal, integrated test functionality and extensive reporting modules&lt;/span>&lt;/li>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>Use of a trusted, secure, fully compliant network with 24/7/365 availability&lt;/span>&lt;/li>&#xa;&lt;/ul>&#xa;"
                                textIsRich="true"/>

                                sling:resourceType="gotomarket/components/content/richtext"
                                text="&lt;ul>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>Full acquiring value chain for dynamic currency conversion transactions, for all channels&lt;/span>&lt;/li>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>Rate provision, transaction eligibility, margin optimization and treasury management&lt;/span>&lt;/li>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>Available on credit and debit card products&lt;/span>&lt;/li>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>Fully compliant with MasterCard and VISA card scheme DCC eligibility rules&lt;/span>&lt;/li>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>80 DCC currencies against EUR basis&lt;/span>&lt;/li>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>Access to equensWorldline’s and FEXCO’s support services for acquirers&lt;/span>&lt;/li>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>Reliable and robust service delivery from PCI-Level 1 DSS partners&lt;/span>&lt;/li>&#xa;&lt;li>&lt;span class=&quot;styled&quot;>Suite of business intelligence reporting&lt;/span>&lt;/li>&#xa;&lt;/ul>&#xa;"
                                textIsRich="true"/>

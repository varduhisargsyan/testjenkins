<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="cq:Page">
    <jcr:content
            jcr:primaryType="nt:unstructured"
            jcr:title="Gotomarket Site Design"
            sling:resourceType="wcm/core/components/designer">
        <page jcr:primaryType="nt:unstructured">
            <content
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="gotomarket/components/parsys"
                    components="[group:Common,group:60-40 Components,group:Layout,/libs/foundation/components/reference]">
                <colctrl
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="gotomarket/components/content/colctrl"
                        layouts="2;cq-colctrl-lt0&#x9;2 Columns (50%, 50%)&#xa;2;cq-colctrl-lt1&#x9;2 Columns (60%, 40%)&#xa;2;cq-colctrl-lt2&#x9;2 Columns (40%, 60%)&#xa;2;cq-colctrl-lt3&#x9;2 Columns (25%, 75%)&#xa;2;cq-colctrl-lt4&#x9;2 Columns (75%, 25%)&#xa;2;cq-colctrl-lt5&#x9;2 Columns (20%, 80%)&#xa;2;cq-colctrl-lt6&#x9;2 Columns (80%, 20%)&#xa;2;cq-colctrl-lt7&#x9;2 Columns (66%, 33%)&#xa;2;cq-colctrl-lt8&#x9;2 Columns (33%, 66%)&#xa;3;cq-colctrl-lt9&#x9;3 Columns (33%, 33%, 33%)&#xa;3;cq-colctrl-lt10&#x9;3 Columns (20%, 60%, 20%)"/>
                <cq:authoring
                        jcr:primaryType="nt:unstructured">
                    <assetToComponentMapping jcr:primaryType="nt:unstructured">
                        <image
                                jcr:primaryType="nt:unstructured"
                                assetGroup="media"
                                assetMimetype="image/*"
                                droptarget="image"
                                resourceType="gotomarket/components/content/image"
                                type="Images"/>
                    </assetToComponentMapping>
                </cq:authoring>
            </content>
        </page>
    </jcr:content>
</jcr:root>

<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:jcr="http://www.jcp.org/jcr/1.0"
          jcr:description="Template for Home/Content Template (without stay in touch) page of the site."
          jcr:primaryType="cq:Template"
          jcr:title="Home/Content Template (without stay in touch)"
          allowedPaths="[/content(/.*)?(/.*)?]"
          ranking="{Long}100">
    <jcr:content
            jcr:primaryType="cq:PageContent"
            carouselItems="\{&quot;imagePath&quot;:&quot;/content/dam/gotomarket/bg-header-solutions.jpg&quot;,&quot;title&quot;:&quot;&quot;,&quot;subtitle&quot;:&quot;&quot;,&quot;description&quot;:&quot;&quot;,&quot;link&quot;:&quot;&quot;}"
            sling:resourceType="gotomarket/components/structure/pages/content">

        <head jcr:primaryType="nt:unstructured"
              sling:resourceType="gotomarket/components/structure/head"/>

        <headlibs jcr:primaryType="nt:unstructured"
                  sling:resourceType="gotomarket/components/structure/headlibs"/>
        <header jcr:primaryType="nt:unstructured"
                sling:resourceType="gotomarket/components/structure/header">
            <leftnavigation
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="gotomarket/components/structure/leftnavigation"/>

            <rightnavigation
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="gotomarket/components/structure/rightnavigation">
                <countrylistdropdown
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="gotomarket/components/structure/countrylistdropdown"/>

                <languagedropdown
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="gotomarket/components/structure/languagedropdown"/>
            </rightnavigation>

            <mainnav
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="gotomarket/components/structure/mainnav"/>

            <infostrip
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="gotomarket/components/structure/infostrip"/>
            <content
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="gotomarket/components/parsys">
                <search
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="gotomarket/components/structure/search"/>
            </content>

            <breadcrumb
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="gotomarket/components/structure/breadcrumb"/>
        </header>

        <headerpage
                jcr:primaryType="nt:unstructured"
                sling:resourceType="gotomarket/components/structure/headerpage"/>

        <contactus jcr:primaryType="nt:unstructured"
                   sling:resourceType="gotomarket/components/structure/contactus"/>

        <footer jcr:primaryType="nt:unstructured"
                sling:resourceType="gotomarket/components/structure/footer">
            <sitemap jcr:primaryType="nt:unstructured"
                     sling:resourceType="gotomarket/components/structure/sitemap"/>
            <followus jcr:primaryType="nt:unstructured"
                      sling:resourceType="gotomarket/components/structure/followus"/>
            <newsletterform jcr:primaryType="nt:unstructured"
                            sling:resourceType="gotomarket/components/structure/newsletterform"/>
        </footer>
        <footlibs jcr:primaryType="nt:unstructured"
                  sling:resourceType="gotomarket/components/structure/footlibs"/>
        <copyright jcr:primaryType="nt:unstructured"
                   sling:resourceType="gotomarket/components/structure/copyright"/>

        <content jcr:primaryType="nt:unstructured"
                 sling:resourceType="gotomarket/components/parsys"/>

    </jcr:content>
</jcr:root>
